<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor
	xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0 http://maven.apache.org/xsd/archetype-descriptor-1.0.0.xsd"
	name="WMS-Archetype-Dubbo-Model"
	xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<requiredProperties>
		<requiredProperty key="db">
			<defaultValue>dubbo</defaultValue>
		</requiredProperty>
	</requiredProperties>
	<modules>
		<module dir="Model">
			<fileSets>
				<fileSet filtered="true" packaged="true" encoding="UTF-8">
					<directory>src/main/java</directory>
					<includes>
						<include>**/*.java</include>
					</includes>
				</fileSet>
				<fileSet filtered="true" packaged="true" encoding="UTF-8">
					<directory>src/test/java</directory>
					<includes>
						<include>**/*.java</include>
					</includes>
				</fileSet>
				<fileSet filtered="true" encoding="UTF-8">
					<directory>src/test/resources</directory>
					<includes>
						<include>**/*.xml</include>
						<include>**/*.properties</include>
					</includes>
				</fileSet>
				<fileSet filtered="true" encoding="UTF-8">
					<directory>src/main/resources</directory>
					<includes>
						<include>**/*.xml</include>
						<include>**/*.properties</include>
					</includes>
				</fileSet>
			</fileSets>
		</module>
		<module dir="Server">
			<fileSets>
				<fileSet filtered="true" packaged="true" encoding="UTF-8">
					<directory>src/main/java</directory>
					<includes>
						<include>**/*.java</include>
					</includes>
				</fileSet>
				<fileSet filtered="true" packaged="true" encoding="UTF-8">
					<directory>src/test/java</directory>
					<includes>
						<include>**/*.java</include>
					</includes>
				</fileSet>
				<fileSet filtered="true" encoding="UTF-8">
					<directory>src/test/resources</directory>
					<includes>
						<include>**/*.xml</include>
						<include>**/*.properties</include>
					</includes>
				</fileSet>
				<fileSet filtered="true" encoding="UTF-8">
					<directory>src/main/resources</directory>
					<includes>
						<include>**/*.Container</include>
						<include>**/*.xml</include>
						<include>**/*.properties</include>
					</includes>
				</fileSet>
			</fileSets>
		</module>
		<module dir="Client">
			<fileSets>
				<fileSet filtered="true" packaged="true" encoding="UTF-8">
					<directory>src/main/java</directory>
					<includes>
						<include>**/*.java</include>
					</includes>
				</fileSet>
				<fileSet filtered="true" packaged="true" encoding="UTF-8">
					<directory>src/test/java</directory>
					<includes>
						<include>**/*.java</include>
					</includes>
				</fileSet>
				<fileSet filtered="true" encoding="UTF-8">
					<directory>src/test/resources</directory>
					<includes>
						<include>**/*.xml</include>
						<include>**/*.properties</include>
					</includes>
				</fileSet>
				<fileSet filtered="true" encoding="UTF-8">
					<directory>src/main/resources</directory>
					<includes>
						<include>**/*.Container</include>
						<include>**/*.xml</include>
						<include>**/*.properties</include>
					</includes>
				</fileSet>
				<fileSet filtered="true" encoding="UTF-8">
					<directory>src</directory>
					<includes>
						<include>wms.conf</include>
					</includes>
				</fileSet>
				<fileSet encoding="UTF-8">
					<directory>src/main/webapp</directory>
					<includes>
						<include>**/*.jsp</include>
						<include>**/*.xml</include>
						<include>**/*.js</include>
						<include>**/*.css</include>
						<include>**/*.png</include>
						<include>**/*.jpg</include>
					</includes>
				</fileSet>
			</fileSets>
		</module>
	</modules>
</archetype-descriptor>
